  // Home / sideBarContent / Content / Slider / Title
  "rich-text#home-sideBarContent-content-slider-title": {
    "props": {
      "blockClass": "home-sideBarContent-content-slider-title",
      "text": "Gift cards **top en ventas**"
    }
  },

  // Home / sideBarContent / Content / Slider / SliderLayout
  "slider-layout#home-sideBarContent-content-slider-images": {
    "props": {
      "blockClass": ["home-sideBarContent-content-slider-images"],
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "autoplay": {
        "timeout": 2000,
        "stopOnHover": true
      }
    },
    "children": [
      "info-card#home-sideBarContent-content-slider-images-items1",
      "info-card#home-sideBarContent-content-slider-images-items2",
      "info-card#home-sideBarContent-content-slider-images-items3",
